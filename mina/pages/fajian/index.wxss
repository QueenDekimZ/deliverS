/*主容器*/
.stepper {
  width: 96px;
  height: 26px;
  margin: 0 auto;
  border: 1px solid #ccc;
  border-radius: 3px;
  float: left;
}

/*加号和减号*/
.stepper text {
  float: left;
  width: 27px;
  line-height: 26px;
  text-align: center;
}

/*数值*/
.stepper input {
  width: 40px;
  height: 26px;
  float: left;
  margin: 0 auto;
  text-align: center;
  font-size: 12px;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
}

/*普通样式*/
.stepper .normal {
  color: black;
}

/*禁用样式*/
.stepper .disable {
  color: #ccc;
}

.button_price_font {
  height: 20%;
